#include<iostream>
using namespace std;

int main()
{
	int n ;
	cout<<"Enter the dimension of the array: "<<endl;
	cin>>n;
	int arr[n];
	cout<<"Start entering the elements of the array"<<endl;
	
	for(int k =0; k<n;k++)
	{
		cin>>arr[k];
	}
	
	int i;
	int max,smax;
	
	for(i=0; i<n; i++)
	{
		max = arr[i];
		for(int j=0; j<n-1;j++)
		{
			if(max<arr[j])
			{
				max=arr[j];
			}
		}
	}
	
	//cout<<max<<endl;
	
	for(i=0; i<n; i++)
	{
		smax = arr[i];
		for(int j=0; j<n-1;j++)
		{
			if(smax<arr[j] && arr[j]!=max)
			{
				smax=arr[j];
			}
		}
	}
	
	//cout<<smax<<endl;
	
	cout<<"Output: "<<smax*smax;
}
